{
  "name": "PassMngr",
  "version": "1.0.0",
  "description": "Secure maintenance to your data..",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-osx": "electron-packager . \"PassMngr\" --platform=darwin --icon ./src/img/icon/icon.icns --out ./build --overwrite",
    "build-linux": "electron-packager . \"PassMngr\" --platform=linux --icon ./src/img/icon/icon.png --out ./build --overwrite",
    "build-win32": "electron-packager . \"PassMngr\" --platform=win32 --icon ./src/img/icon/icon.ico --out ./build --overwrite",
    "build": "electron-packager . \"PassMngr\" --platform=all --icon ./src/img/icon/icon --out ./build --overwrite"
  },
  "author": "SKH Engineering Consultants, Inc.",
  "license": "MIT",
  "devDependencies": {
    "electron": "^15.5.5",
    "electron-winstaller": "^2.7.0",
    "electron-wix-msi": "^1.3.0"
  },
  "dependencies": {
    "nedb": "^1.8.0"
  }
}
